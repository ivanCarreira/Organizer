<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.organizerv02"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.organizerv02.Organizer"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.organizerv02.NuevaTarea"
            android:label="@string/title_activity_nueva_tarea"
            android:parentActivityName="com.example.organizerv02.Organizer" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.organizerv02.Organizer" />
        </activity>
        <activity
            android:name="com.example.organizerv02.Preferencias"
            android:label="@string/title_activity_preferencias" >
        </activity>
    </application>

</manifest>
